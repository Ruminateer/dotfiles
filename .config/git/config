	oldNormal = red dim
	oldHighlight = red
	newNormal = green dim
	newHighlight = green
[init]
	defaultBranch = master
[fetch]
	prune = true
[alias]
	a = add
	b = branch
	c = commit
	d = diff
	f = fetch
	l = log --graph --date=human --pretty=format:'%C(auto)%h %Cblue%ad %Cgreen%aN %C(auto)%s%d'
	s = status

	co = checkout
	mg = merge
	pl = pull
	ps = push
	rb = rebase
	rs = reset
	rv = revert
	sh = show
	sw = switch

