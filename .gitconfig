[user]
	email = zhangzhengyuan0604@gmail.com
	name = zhangzhengyuan
[core]
	autocrlf = false
	editor = vim
	excludesFile = ~/default.gitignore
	pager = perl /usr/share/doc/git/contrib/diff-highlight/diff-highlight | less
[credential]
	helper = store
[color "diff"]
	context = normal dim
[color "diff-highlight"]
	oldNormal = red dim
	oldHighlight = red
	newNormal = green dim
	newHighlight = green
[init]
	defaultBranch = master
[alias]
	a = add
	b = branch
	c = commit
	d = diff
	f = fetch --prune
	l = log --oneline --graph -20
	s = status

	co = checkout
	mg = merge
	pl = pull
	ps = push
	rb = rebase
	rs = reset
	rv = revert
	sh = show
	sw = switch

	cl = clean -fd
	cla = cl -x
	rv = diff --staged
	uc = reset --soft HEAD^
